        <TableView VBox.vgrow="SOMETIMES" fx:id="table" />
        <VBox  spacing="5" >
            <children>
                <HBox>
                    <children>
                        <Parameter fx:id="coordinatesParameter" name="Coordinates" type="TEXT" editable="false" />
