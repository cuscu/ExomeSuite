                        <Parameter fx:id="rsidParameter" name="Identifier" type="TEXT" editable="false"/>
                        <Parameter fx:id="qualParameter" name="Quality" type="TEXT" editable="false"/>
                        <Parameter fx:id="filterParameter" name="Filter" type="TEXT" editable="false"/>
                    </children>
                </HBox>
                <HBox fx:id="infoBox" />
