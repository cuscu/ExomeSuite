<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import exomesuite.graphic.*?>

<fx:root type="javafx.scene.layout.VBox" xmlns:fx="http://javafx.com/fxml/1" alignment="CENTER_LEFT" >
    <children>
        <HBox fx:id="buttons" />
        <Label fx:id="message" text="Click on an action."  />
        <HBox alignment="CENTER"  >
            <children>
                <ProgressBar fx:id="progressBar" maxWidth="999999" HBox.hgrow="ALWAYS" />
                <FlatButton icon="cancel4.png" fx:id="cancel"/>
            </children>
        </HBox>
    </children>
</fx:root>
