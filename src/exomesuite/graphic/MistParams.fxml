<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import exomesuite.graphic.*?>

<VBox  xmlns:fx="http://javafx.com/fxml/1" fx:controller="exomesuite.graphic.MistParams" alignment="TOP_CENTER" prefWidth="400"  >
    <children>
        <Parameter fx:id="bamFile" name="Alignments file" type="OPTIONS" value="Select a BAM file"/>
        <Parameter fx:id="threshold" name="Threshold" type="TEXT" value="10" />
        <Parameter fx:id="length" name="Length" type="TEXT" value="1"/>
        <Button text="Accept" fx:id="accept"/>
    </children>
    <padding>
        <javafx.geometry.Insets bottom="5" left="5" right="5" top="5" />
    </padding>
</VBox>
