<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import exomesuite.graphic.*?>
<?import java.net.*?>

<fx:root type="javafx.scene.layout.VBox" spacing="5"  xmlns:fx="http://javafx.com/fxml/1" maxHeight="600" >
    <stylesheets>
        <URL value="@/exomesuite/main.css"/>
    </stylesheets>
    <children>
        <!-- Standard databases -->
        <SectionTitle title="%variant.databases"/>
        <FileParam  title="%mills" fx:id="mills"/>
        <FileParam title="%phase1" fx:id="phase1" />
        <FileParam title="%omni" fx:id="omni" />
        <FileParam title="%hapmap" fx:id="hapmap" />
        <FileParam title="%dbsnp" fx:id="dbsnp" />
        <FileParam title="%ensembl.exons" fx:id="ensembl"/>
        <!-- Reference genomes -->
        <SectionTitle title="%reference.genome" />
        <FileParam title="GRCHv37 (hg19)" fx:id="grch37"/>
        <FileParam title="GRCHv38" fx:id="grch38" />

    </children>
</fx:root>
